<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/css/bootstrap.min.css">
<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.10.3/font/bootstrap-icons.css">
<link rel="stylesheet" href="stylesheet/style.css">

<title>HSCode</title>

<head>
  <%- include('partials/dashboard'); %>
</head>

<div class = "container3" style ="margin-left: 100px; margin-right: 100px">
  <h1>HSCode</h1>
  <a href="https://tradingacrossborders.mofe.gov.bn/PublishingImages/SitePages/home/BDTTC%202022.pdf">All HSCode are based on Tariff 2022 under MOFE</a>
  <form action="/addhscode" method="POST">
  
    <div class="row">
      <div class="col leftcolumn">
        <table class="table table-striped table-bordered table-sm">
          <thead>
            <tr>
                <th scope="col">Heading</th>
                <th scope="col">HSCode</th>
                <th scope="col">Description</th>
                <th scope="col">Quantity</th>
                <th scope="col">Rate of Import Duty</th>
                <th scope="col">Rate of Excise Duty</th>
            </tr>
          </thead>
          <tbody>
            <tr>
              <th><%= hscode[0].Heading %></th>
              <td><%= hscode[0].HSCode %></td>
              <th><%= hscode[0].Description %></th>
              <th><%= hscode[0].Quantity %></th>
              <td><%= hscode[0].ImportDutyRate %></td>
              <td><%= hscode[0].ExciseDutyRate %></td>
          </tr>
            <% for (var i = 1; i<hscode.length; i++) { %>
            <tr>
              <td><%= hscode[i].Heading %></td>
              <td><%= hscode[i].HSCode %></td>
              <td><%= hscode[i].Description %></td>
              <td><%= hscode[i].Quantity %></td>
              <td><%= hscode[i].ImportDutyRate %></td>
              <td><%= hscode[i].ExciseDutyRate %></td>
            </tr>
            <% } %>
          </tbody>
        </table>
      </div>

    </div>

</form>



</div>

<footer>
  <%- include('partials/footer'); %>
</footer>